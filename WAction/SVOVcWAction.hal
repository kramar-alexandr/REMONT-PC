procedure AddCRToNoter(string tstr, var record NotepadVc Noter)
begin
	integer i;
	
	For(i=0;i<len(tstr);i=i+1) begin
	  if(asc(mid(tstr,i,1))==92)then begin
	  	if(i<len(tstr)-1)then begin
	  		if(mid(tstr,i+1,1)=="n" or mid(tstr,i+1,1)=="N")then begin
	  			AddToText(chr(13) & chr(10),Noter);
	  			i=i+1;
	  		end else begin
	  			AddToText(mid(tstr,i,1),Noter);
	  		end;
	  	end;
	  end else begin
	  	AddToText(mid(tstr,i,1),Noter);
	  end;
	end; 

return;
end;

global function Boolean NotepadDClassstandartproblemOnEnterKey(Integer wn,Integer rownr)
begin
  Boolean res;
  record NotepadVc Noter;
  record StandProblemVc SPr;
  string 100 input;
		
  input = GetWindowString(wn,"standartproblem");
  PutWindowString(wn,"standartproblem","");
  DeselectWindow(wn,false);
  GetWindowRecord(wn,Noter);
	
	SPr.Code = input;
	if(readfirstmain(SPr,1,true))then begin
		if(nonblank(SPr.Comment1))then begin
			AddCRToNoter(SPr.Comment1,Noter);
		end;
		if(nonblank(SPr.Comment2))then begin
			AddCRToNoter(SPr.Comment2,Noter);
		end;
		if(nonblank(SPr.Comment3))then begin
			AddCRToNoter(SPr.Comment3,Noter);
		end;
	end;
  
  putwindowrecord(wn,Noter);    
  WindowFieldGoto(wn,Noter,-1,"standartproblem",false);
  
  NotepadDClassstandartproblemOnEnterKey = false;
  return;
end;

global
function Boolean NotepadDClassOnEnterKey(Integer wn,string fieldname,Integer fn,Integer rownr)
BEGIN
  Boolean res;

	res = true;
	switch (fieldname) begin
		case "standartproblem":
			res = NotepadDClassstandartproblemOnEnterKey(wn,rownr);
	end;
  NotepadDClassOnEnterKey = res;
  RETURN;
END;