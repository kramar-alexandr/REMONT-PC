function Integer FindInMathOR(var record RcVc RepSpec,var string field)
begin
  record ORVc ORr;
  row ORVc ORrw;
  boolean continue;
  Integer i,rwcnt,mwn,res,wn;
  string 20 search;
  
  //wn = CurWindow; //ORTableSearchIClass
  mwn = RepSpec.flags[0];
  search = RepSpec.f1;
  GetWindowRecord(mwn,ORr);
  rwcnt = MatRowCnt(ORr);
  res=RepSpec.flags[3]; //last result
  field="";

	if (RepSpec.flags[2]==1) then begin  //forward search
		  for (i = RepSpec.flags[1];i<rwcnt;i=i+1) begin

					MatRowGet(ORr,i,ORrw);
						if(FindSubString(search,ORrw.ArtCode)) then begin
							field = "ArtCode";
							res=i;
							goto LFindInMathOR;
						end else begin
						
							if(FindSubString(search,ORrw.Spec)) then begin
								field = "Spec";
								res=i;
								goto LFindInMathOR;
							end else begin
				
								if(FindSubString(search,ORrw.Price)) then begin
									field = "Price";
									res=i;
									goto LFindInMathOR;
									end;
								end;
								end;
			  
		  end;  
	
	end else begin

	if (RepSpec.flags[2]==2) then begin  //backward search
		  for (i = RepSpec.flags[1];i>=0;i=i-1) begin

					MatRowGet(ORr,i,ORrw);
						if(FindSubString(search,ORrw.ArtCode)) then begin
							field = "ArtCode";
							res=i;
							goto LFindInMathOR;
						end else begin
						
							if(FindSubString(search,ORrw.Spec)) then begin
								field = "Spec";
								res=i;
								goto LFindInMathOR;
							end else begin
				
								if(FindSubString(search,ORrw.Price)) then begin
									field = "Price";
									res=i;
									goto LFindInMathOR;
									end;
								end;
								end;						 
			end;	  
		
		  end;
	end;

  if ((field =="") ) then begin
  MessageBox(0,"Не знайдено результатів для " & search);
  end;

  LFindInMathOR:;	
  FindInMathOR=res;
return;
end;



