procedure CloseActivity(var record ActVc Actr, string actResult)
begin
    record ActVc lActr;
    recordcopy(lActr,Actr);
    Actr.ActResult = actResult;
    Actr.OKFlag = 1;
    recordupdate(lActr,Actr,true);
end;

