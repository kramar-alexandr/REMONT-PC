    MessageBox(23700,"");
    goto LSendToAppleSVODsm;
  end;
  
  BlockLoad(GSb);
  if (GSXLogin(GSb.AppleID,GSb.Password,GSb.AccountNo,sessionID,errormsg)) then begin
