      	UpdateLoyCard(SVOr,SVO2r);// Edit ************************** BPI Ukraine - KramarAlexandr - 04, 15 03 2018 y. о 19:51:40
        RecordStore(SVOr,true);
      end;
    end;
  end;
  return;
end; //Edit***************************Sasha2,14:33 15.08.2017 }

