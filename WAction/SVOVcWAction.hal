global updating procedure ChangeSVOSalesGroup()
begin
	record UserVc USr;
	record SVOVc SVOr;
	integer wn;
	integer state;
	
	wn = curwindow;
	state = windowstate(wn);
	
	if(state==Rs_normal)then begin
		USr.Code = currentuser;
	
		if(readfirstmain(USr,1,true))then begin
			if(nonblank(USr.SalesGroup))then begin
				getwindowrecord(wn,SVOr);
