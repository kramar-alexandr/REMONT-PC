  record SVOVc SVOr,SVO2r;
  record IVVc IVr;
  row IPVc IPrw;
  Integer i,rwcnt;
  val sumToSVO;
  string 20 payField;
  Boolean saveF,prepaymentF;
  
  rwcnt = MatRowCnt(IPr);
  for (i=0;i<rwcnt;i=i+1) begin
